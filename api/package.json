{
  "name": "adapoint-api",
  "version": "1.0.0",
  "description": "Adapoint Web API",
  "author": "Satrio A. Cesiojakty <satrioarbiyudho@gmail.com>",
  "license": "BSD",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon",
    "prod": "node ./",
    "moge": "sequelize model:generate --name",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "drop": "sequelize db:drop",
    "mimu": "sequelize db:migrate:undo",
    "createdb": "sequelize db:create",
    "cremi": "yarn drop && yarn createdb && yarn migrate"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.1.0",
    "axios": "^0.21.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "fast-csv": "^4.3.6",
    "form-data": "^4.0.0",
    "get-folder-size": "^2.0.1",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-graceful-shutdown": "^1.2.1",
    "koa-static": "^5.0.0",
    "node-cache": "^5.1.2",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.12",
    "sequelize-cli": "^6.2.0"
  }
}
